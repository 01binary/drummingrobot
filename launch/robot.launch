<launch>
  <node name="robot" pkg="str1ker" type="robot" required="true" output="screen">
    <rosparam>
      arms:
        arm1:
          shoulder:
            controller: 'servo'
            enable: true
            lpwm: 13
            rpwm: 26
            time: 5
          upperarm:
            controller: 'linear'
            enable: false
            lpwm: 25
            rpwm: 24
            time: 5
          forearm:
            controller: 'linear'
            enable: false
            lpwm: 18
            rpwm: 17
            time: 4
          trigger:
            controller: 'solenoid'
            enable: false
            output: 23
    </rosparam>
  </node>
</launch>
